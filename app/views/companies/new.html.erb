
<%= form_for @company do |f| %>

  <%= f.label :name %><br>
  <%= f.text_field :name %><br>

  <%= f.label :url %><br>
  <%= f.text_field :url %><br>

  <%= f.label :city %><br>
  <%= f.text_field :city %><br>

  <%= f.label :state %><br>
  <%= f.text_field :state %><br>

  <%= f.label :about %><br>
  <%= f.text_area :about %><br>
 
  <%= f.submit %>
<% end %>
