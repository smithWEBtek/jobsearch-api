<div>
  <table>
    <th>
      <h4><tr align="left"><strong>
        <td>priority</td>
        <td>date</td>
        <td>step</td>
        <td>notes</td>
        <td>company</td>
        <td>contact1</td>
        <td>contact2</td>
        <td>job</td>
      </strong></tr></h4>
    </th>
    <% todos.each do |todo| %>
    <tr>
      <td><%= todo.priority %></td>
      <td><%= todo.date %></td>
      <td><%= todo.step.name %></td>
      <td><%= todo.notes %></td>
      <td><%= link_to todo.company.name, company_path(todo.company) %></td>
      <td><%= link_to todo.contact.fname.concat(todo.contact.lname), contact_path(todo.contact) %></td>
      <td><%= link_to todo.job.title, job_path(todo.job) %></td>
    </tr>
    <% end %>
  </table>
</div>
